'use strict';Object.defineProperty(exports, "__esModule", { value: true });var _auth = require('../actions/auth');







const initialState = {
    authToken: null,
    currentUser: null,
    error: null,
    signedUp: null };


const loginReducer = (state = initialState, action) => {
    switch (action.type) {
        case _auth.SET_AUTH_TOKEN:
            return Object.assign({},
            state, {
                authToken: action.authToken });

        case _auth.CLEAR_AUTH:
            return Object.assign({},
            state, {
                authToken: null,
                currentUser: null });

        case _auth.AUTH_SUCCESS:
            return Object.assign({},
            state, {
                currentUser: action.currentUser,
                signedUp: false,
                error: null });

        case _auth.AUTH_ERROR:
            return Object.assign({},
            state, {
                error: action.error });

        case _auth.SIGNED_UP:
            return Object.assign({},
            state, {
                signedUp: true });

        default:
            return state;}

};exports.default =

loginReducer;